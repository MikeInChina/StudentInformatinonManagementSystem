<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wdc.studentinformationmanagementsystem.ChooseColController">
   <children>
      <Label text="选取对应列">
         <font>
            <Font name="Microsoft YaHei UI" size="18.0" />
         </font>
         <padding>
            <Insets top="10.0" />
         </padding>
      </Label>
      <HBox alignment="CENTER" prefHeight="42.0" prefWidth="400.0">
         <children>
            <Label text="学号：">
               <font>
                  <Font name="Microsoft YaHei UI" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="numberBox" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="42.0" prefWidth="400.0">
         <children>
            <Label text="姓名：">
               <font>
                  <Font name="Microsoft YaHei UI" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="nameBox" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="42.0" prefWidth="400.0">
         <children>
            <Label text="性别：">
               <font>
                  <Font name="Microsoft YaHei UI" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="genderBox" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="42.0" prefWidth="400.0">
         <children>
            <Label text="班级：">
               <font>
                  <Font name="Microsoft YaHei UI" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="classBox" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="42.0" prefWidth="400.0" spacing="100.0">
         <children>
            <Button mnemonicParsing="false" onAction="#ok" text="确定">
               <font>
                  <Font name="Microsoft YaHei UI" size="12.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#cancel" text="取消">
               <font>
                  <Font name="Microsoft YaHei UI" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
